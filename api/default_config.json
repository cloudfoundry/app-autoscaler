      }
    },
    "broker_server": {
      "port": 0
    },
    "scaling_rules": {
      "cpu": {
        "lower_threshold": 1,
        "upper_threshold": 100
      },
      "cpuutil": {
        "lower_threshold": 1,
        "upper_threshold": 100
